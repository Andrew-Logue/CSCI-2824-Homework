   "source": [
    "# Andrew Logue\n",
    "# CSCI 2824\n",
    "\n",
    "def sequence_slayer(N):\n",
    "    a0 = 1\n",
    "    a1 = 2\n",
    "    if N == 0:\n",
    "        return 1\n",
    "    elif N == 1:\n",
    "        return 2\n",
    "    for i in range(N-1):\n",
    "        aN = (((i+2)**2)*a1)-(a0)\n",
    "        a0 = a1\n",
    "        a1 = aN\n",
    "        \n",
    "    return aN\n",
    "\n",
    "sequence_slayer(8)\n"
   ]
