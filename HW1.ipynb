   "source": [
    "# Andrew Logue\n",
    "# CSCI 2824\n",
    "\n",
    "print(\"hello world\");\n",
    "\n",
    "def bitOperator(x,y):\n",
    "    byte1 = int(x,2) | int(y,2)\n",
    "    byte2 = int(x,2) & int(y,2)\n",
    "    tuple = (format(byte1,'b'), format(byte2,'b'))\n",
    "    return tuple\n",
    "\n",
    "\n",
    "bitOperator('01001','00100')\n",
    "\n"
   ]
