    "# Andrew Logue\n",
    "# CSCI 2824\n",
    "\n",
    "def first_D_digit_Lucas(D):\n",
    "    L0 = 2\n",
    "    L1 = 1\n",
    "    LN = 0\n",
    "    if D == 1:\n",
    "        return 2\n",
    "    while len(str(LN)) != D:\n",
    "        LN = L0 + L1\n",
    "        L0 = L1\n",
    "        L1 = LN\n",
    "        \n",
    "    return LN\n",
    "\n",
    "first_D_digit_Lucas(3)\n"
   ]
